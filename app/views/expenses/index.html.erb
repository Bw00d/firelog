<p id="notice"><%= notice %></p>

<%= render 'form' %>

<h6>Expenses</h6>

<div id="expenses">
    <% @expenses.each do |expense| %>
      <div class="expense small-2 large-4 columns"><%= expense.date %></div>
      <div class="expense small-4 large-4 columns"><%#= expense.category_id %></div>
      <div class="expense small-6 large-4 columns"><%#= expense.vendor_id %></div>
      <div class="expense small-6 large-4 columns"><%= expense.amount %></div>
    <% end %>
</div>
